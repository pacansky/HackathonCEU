    "\n",
    "data.rename(columns={\"is_bonus\": \"n_bonuses\", \"is_penalty\": \"n_penalties\"}, inplace=True)\n",
    "\n",
    "data['r_time'] = pd.to_datetime(data['registration_time'])\n",
    "data['days_from_registr'] = pd.to_datetime(data['r_time'].max()) - data['r_time']\n",
